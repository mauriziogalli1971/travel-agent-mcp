{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "mcp-client",
	"main": "src/index.js",
	"compatibility_date": "2025-09-06",
	"compatibility_flags": [
		"nodejs_compat"
	],
	"observability": {
		"enabled": false,
		"logs": {
			"enabled": false
		}
	},
	"dev": {
		"port": 8787
	},
	"env": {
		"local": {
			"vars": {
				"CLOUDFLARE_INCLUDE_PROCESS_ENV": true
			}
		},
		"production" : {
			"services": [
				{
					"binding": "MCP_SERVER",
					"service": "mcp-server-production"
				}
			]
		}
	}
}
